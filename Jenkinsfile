pipeline {
  agent any
  stages {
    stage ("build") {
      step {
        echo 'building application...'
      }
    }
    stage ("test") {
      step {
        echo 'testing the application...'
      }
    }
    stage ("deploy") {
      step {
        echo 'deployment compleated...'
      }
    }
  }
}


    
